<!-- Display an embedded YouTube player -->

<iframe
  width="560",
  height="315",
  src="<%= url %>",
  title="YouTube video player",
  showinfo="0",
  frameborder="0",
  controls="0",
  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture",
  allowfullscreen>
</iframe>
