<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>[Projects]</h1>

      <ul>
        <% @projects.each do |p| %>
          <li>
            <%= p.title %>
            <%= cl_image_tag(p.photo.key, height: 400, width: 400) if p.photo.attached? %>
          </li>
        <% end %>
      </ul>

    </div>
  </div>
</div>
